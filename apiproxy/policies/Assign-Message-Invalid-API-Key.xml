<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="Assign-Message-Invalid-API-Key">
    <DisplayName>Assign-Message-Invalid-API-Key</DisplayName>
    <Properties/>
