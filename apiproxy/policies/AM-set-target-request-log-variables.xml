    <Properties/>
    <AssignVariable>
        <Name>log.target.request.verb</Name>
        <Ref>request.verb</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>log.target.request.header.host</Name>
        <Ref>request.header.host</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>log.target.request.url</Name>
